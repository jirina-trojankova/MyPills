    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.9",
    "html-webpack-plugin": "^3.2.0",
    "if-env": "^1.0.0",
    "node-sass": "^4.7.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.8.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"

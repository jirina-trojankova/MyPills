    "promise-polyfill": "^8.0.0",
    "prototype-indexof-shim": "^1.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux": "^4.0.0",
    "redux-create-reducer": "^1.1.2",
    "redux-thunk": "^2.3.0",

{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "member-ordering": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": false,
    "no-empty-interface": false
  }
}
